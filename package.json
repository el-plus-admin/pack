{
  "name": "@kikiutils/kiki-core-stack-pack",
  "version": "0.14.0",
  "type": "module",
  "scripts": {
    "build": "ts-project-builder './src/**/*.ts' -f esm --clean --preserve-modules",
    "bumplog": "changelogen --bump"
  },
  "dependencies": {
    "@kikiutils/mongoose": "^1.3.1",
    "@kikiutils/node": "^4.2.4",
    "@kikiutils/types": "^2.2.1",
    "@types/lodash-es": "^4.17.12",
    "@types/nodemailer": "^6.4.16",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.7.7",
    "h3": "^1.12.0",
    "ioredis": "^5.4.1",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.15",
    "otp-io": "^1.2.6"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "changelogen": "^0.5.7",
    "date-fns": "^4.1.0",
    "mongoose": "^8.7.0",
    "nitropack": "^2.9.7",
    "rollup-plugin-delete": "^2.1.0",
    "ts-project-builder": "^3.0.1",
    "tslib": "^2.7.0"
  },
  "exports": {
    "./constants": {
      "import": "./constants/index.mjs",
      "types": "./constants/index.d.ts"
    },
    "./constants/*": {
      "import": "./constants/*.mjs",
      "types": "./constants/*.d.ts"
    },
    "./controllers/*": {
      "import": "./controllers/*.mjs",
      "types": "./controllers/*.d.ts"
    },
    "./models": {
      "import": "./models/index.mjs",
      "types": "./models/index.d.ts"
    },
    "./nitropack/*": {
      "import": "./nitropack/*.mjs",
      "types": "./nitropack/*.d.ts"
    },
    "./nitropack/utils": {
      "import": "./nitropack/utils/index.mjs",
      "types": "./nitropack/utils/index.d.ts"
    },
    "./types": {
      "types": "./types/index.d.ts"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    },
    "./types/data": {
      "types": "./types/data/index.d.ts"
    },
    "./utils": {
      "import": "./utils/index.mjs",
      "types": "./utils/index.d.ts"
    },
    "./utils/*": {
      "import": "./utils/*.mjs",
      "types": "./utils/*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
